{
  "mcpServers": {
    "genkit": {
      "type": "stdio",
      "command": "genkit",
      "args": [
        "mcp"
      ],
      "env": {}
    },
    "chrome-devtools-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "env": {}
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "ide-assistant"
      ],
      "env": {}
    }
  }
}